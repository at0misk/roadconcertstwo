function fadeCB(callback){
  $('.formOut').fadeOut(600);
  callback;
}

function fadeIn(){
  $('.formIn').fadeIn(600);
}

window.onbeforeunload = function () {
  window.scrollTo(0, 0);
}
    $(document).ready(function(){
      $('.parallax').parallax();
      $('.scrollspy').scrollSpy();
      $('.tabs-wrapper .row').pushpin({ top: $('.tabs-wrapper').offset().top });
        var options = [
        {selector: '.first-text', offset: 600, callback: function(){
          $('.tabs-wrapper').fadeIn(1100);
          console.log('hit');
        }},
        {selector: '.first-text', offset: 400, callback: function() {
          Materialize.showStaggeredList($('#staggered'));
        }},
        {selector: '.second-text', offset: 400, callback: function() {
          Materialize.showStaggeredList($('#second-staggered'));
        }},
        {selector: '#bottom', offset: 0, callback: function(){
          $('.twittcont').removeClass('noscroll');
          console.log('hit bottom');
        }}
      ];
      Materialize.scrollFire(options);
      $('#mailerform').on('submit', function(){
      var valuesToSubmit = $(this).serialize();
      $.ajax({
          type: "POST",
          url: "/mail", //sumbits it to the given url of the form
          data: valuesToSubmit,
          success: function(result){
            $('.formOut').fadeOut(600, function(){
              $('.formIn').fadeIn(600);
            });
          } // you want a difference between normal and ajax-calls, and json is standard
      })
      return false; // prevents normal behaviour
    });
      $('#pr').on('click', function(){
        $('.fl1').click();
      })
      $('#cf').on('click', function(){
        $('.fl2').click();
      })
    });
   </script>
